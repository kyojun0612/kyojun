import random
rn = random.randrange(1,101,1)
t_cnt = 0
print("Up & down 게임시작")
print("--------------------")
while ( rn != num) :
    num = int(input("1~100 숫자 : "))

    if (num > rn) :
      print("Down")
    elif (num < rn) :
      print("up")

    t_cnt += 1
print("------------------------")
print(t_cnt, "번 만에 정답")
