package kkj;

import java.util.Scanner;

import java.util.Scanner;

public class Kkj2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("야 숫자 하나 입력해봐");
		int num = sc.nextInt();
		
		if(num%2==0)
		   System.out.println("짝수입니다");
         
		 else
			   System.out.println("홀수입니다");
	}

}
