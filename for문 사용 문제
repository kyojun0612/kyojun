package kkj;

import java.util.Scanner;

public class Kkj612 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("숫자 하나만 입력해봐~");
		
	    for(int num=sc.nextInt(); num>0; num=num/10) {
	    	System.out.println(num % 10);
	    	
	    
		
	    }
	}

}
